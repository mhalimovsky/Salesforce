<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="classes" thread-count="1">
    <listeners>
        <listener class-name="tests.AnnotationTransformer"/>
        <listener class-name="tests.TestListener"/>
    </listeners>
    <test name="Account">
        <classes>
            <class name="tests.NewAccountTest"/>
        </classes>
    </test>
    <test name="Contact">
        <classes>
            <class name="tests.NewContactTest"/>
        </classes>
    </test>
    <test name="Login">
        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>
</suite>